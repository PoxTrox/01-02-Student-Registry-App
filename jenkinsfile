pipeline {
    agent any 
    stages {
        stage('Install Dependencies') { 
            steps {
                bat 'npm install' 
            }
        }
        stage('Execute tests') { 
            steps {
                script {
                    input message: 'Do you want to start the Integration test', ok: 'Run Tests'
                    bat 'npm run test'
                } 
            }
        }
    }
}